 	function autocomplete(){
        
            var url = '{{ path('admin_event_result_not_asigne_all' , { 'search' :'search_value'}) }}';
            var value = $( "#search_value" ).val();
          
            var url_value = url.replace('search_value', encodeURIComponent(value));
            
            $('#classifications').load(url_value);
    }
    
    function autocomplete_mobile(){
        

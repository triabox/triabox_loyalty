						<div class="row">
							<div class="col-xs-12">
								<header id="login-header">
									<div id="login-logo">
										
									</div>
								</header>
								
								<div id="login-box-inner">
									{% trans %}Login or Registration{% endtrans %}
									{% block form_login %}
													
									{% endblock %}
									
									
									
								</div>
							</div>
						</div>
					</div>
					{% block form_login_footer %}
													
					{% endblock %}
					
					

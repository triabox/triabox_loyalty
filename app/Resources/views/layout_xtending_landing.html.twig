    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">{% trans %}contact.us{% endtrans %}</h2>
                    <h3 class="section-subheading text-muted"> </h3>
                </div>
            </div>
            <div class="row">
            	<div id="output">
            	</div>
                <div class="col-lg-12">
                    <form action="#" name="contactoForm" id="contactoForm" method="post" novalidate>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="{% trans %}your.name{% endtrans %}" id="contacto_name" name="contacto_name" required data-validation-required-message="Please enter your name.">
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    <input type="email" class="form-control" placeholder="{% trans %}your.email{% endtrans %}" id="contacto_email" name="contacto_email" required data-validation-required-message="Please enter your email address.">
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    <input type="tel" class="form-control" placeholder="{% trans %}your.phone{% endtrans %}" id="contacto_phone" name="contacto_phone" required data-validation-required-message="Please enter your phone number.">
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <textarea class="form-control" placeholder="{% trans %}your.message{% endtrans %}" id="contacto_message" name="contacto_message" required data-validation-required-message="Please enter a message."></textarea>
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-lg-12 text-center">
                                <div id="success"></div>
                                <a type="submit" class="btn btn-xl" href="#" onclick="enviarConsulta();return false;">{% trans %}send.message{% endtrans %}</a>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>

<!DOCTYPE html>
<html>
<head>
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="http://code.jquery.com/mobile/1.4.2/jquery.mobile-1.4.2.min.css">
<script src="http://code.jquery.com/jquery-1.10.2.min.js"></script>
<script src="http://code.jquery.com/mobile/1.4.2/jquery.mobile-1.4.2.min.js"></script>

		<link rel="stylesheet" href="../css/themes/default/jquery.mobile-1.4.3.min.css">
		<link rel="stylesheet" href="../_assets/css/jqm-demos.css">
{# 		<link rel="shortcut icon" href="../favicon.ico">#}
		<link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,700">
		
		
{% block stylesheets %}
		{% stylesheets filter='cssrewrite'
			'bundles/frontend/jqueryMobile/demos/css/themes/default/jquery.mobile-1.4.3.min.css'
			'bundles/frontend/jqueryMobile/demos/_assets/css/jqm-demos.css' 
		%}
		
		    <link rel="stylesheet" href="{{ asset_url }}" />
		{% endstylesheets %}
		<!--[if IE 7]>
		<link rel="stylesheet" type="text/css" href="/xtending/web/bundles/frontend/css/ie7.css" />
	   <![endif]-->	
{% endblock %}

</head>
<body>
{% block body %}
<div data-role="page" id="pageone">
  <div data-role="header">
    <h1>Full Width Collapsible Lists</h1>
  </div>

  <div data-role="main" class="ui-content">
    <h2>My Phonebook</h2>
    <div data-role="collapsible" data-inset="false">
    <h4>A</h4>
    <ul data-role="listview">
      <li><a href="#">Adele</a></li>
      <li><a href="#">Agnes</a></li>
    </ul>
    </div>

    <div data-role="collapsible" data-inset="false">
    <h4>B</h4>
    <ul data-role="listview">
       <li><a href="#">Billy</a></li>
       <li><a href="#">Bob</a></li>
    </ul>
    </div>

    <div data-role="collapsible" data-inset="false">
    <h4>C</h4>
    <ul data-role="listview">
      <li><a href="#">Calvin</a></li>
      <li><a href="#">Cameron</a></li>
      <li><a href="#">Chloe</a></li>
      <li><a href="#">Christina</a></li>
    </ul>
    </div>
  </div>

  <div data-role="footer">
    <h1>Insert Footer Text Here</h1>
  </div>
</div> 
{% endblock %}


    {% block script %}
    
			{% javascripts 
				'bundles/frontend/jqueryMobile/demos/js/jquery.mobile-1.4.3.min.js'
				'bundles/frontend/jqueryMobile/demos/_assets/js/index.js'
				'bundles/frontend/jqueryMobile/demos/js/jquery.js'
			
			output="scripts.js" %}
		    	<script type="text/javascript" src="{{ asset_url }}"></script>
			{% endjavascripts %}
			<script type="text/javascript">
				{% block script_custom %}
				
				{% endblock %}
				{% block script_custom_ejecute %}
				
				{% endblock %}
			</script>
	   {% endblock %}
</body>
</html>

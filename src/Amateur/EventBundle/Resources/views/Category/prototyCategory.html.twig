<div class="col-lg-12">
		<div class="main-box">
			<div class="main-box-body clearfix">
			<div class="row">
				<div class="form-group col-xs-3">
					<label for="datepickerDate">{% trans %}Nombre{% endtrans %}</label>
					<div class="input-group">
						<input type="text" class="form-control" id="datepickerDate" 
						value="{{ categories.name.vars.value }}"
						name="{{ categories.name.vars.full_name }}">
					</div>
				</div>
				<div class="form-group col-xs-3">
					<label for="datepickerDateRange">{% trans %}Identificador{% endtrans %}</label>
					<div class="input-group">
						<input type="text" class="form-control" id="datepickerDate" 
						value="{{ categories.code.vars.value }}"
						name="{{ categories.code.vars.full_name }}">
					</div>
				</div>
				<div class="form-group col-xs-3">
					<label for="timepicker">{% trans %}Natacion{% endtrans %}</label>
					<div class="input-group input-append bootstrap-timepicker">
						<div class="form-group form-group-select2 col-xs-4"">
						{{ form_widget(categories.distanceSwim) }}
							
						</div>
					</div>
				</div>
				<div class="form-group col-xs-3">
					<label for="datepickerDateComponent">{% trans %}Bicicleta{% endtrans %}</label>
					<div class="input-group" >
						<div class="form-group form-group-select2 col-xs-4"">
						{{ form_widget(categories.distanceBike) }}
						
						</div>
					</div>
				</div>
				<div class="form-group col-xs-3">
					<label for="timepicker">{% trans %}Carrera{% endtrans %}</label>
					<div class="input-group input-append bootstrap-timepicker">
						<div class="form-group form-group-select2 col-xs-4">
						{{ form_widget(categories.distanceFirstRun) }}
						</div>
					</div>
				</div>
				<div class="form-group col-xs-3">
					<label for="datepickerDateComponent">{% trans %}Carrera 2{% endtrans %}</label>
					<div class="input-group" >
						<div class="form-group form-group-select2 col-xs-4">
						{{ form_widget(categories.distanceLastRun) }}
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

																											
